{{#if model}}
  <div class="panel panel-transparent">
    <div class="panel-body">    
      <div class="col-md-9 col-md-offset-3">
        <div class="alert alert-danger" role="alert">
          Are you sure you want to remove prefix {{model.prefix.id}} from this member?
        </div>
        <div class="panel-footer">
          <div class="btn-toolbar pull-right">
            <button id="cancel" class="btn btn-sm" type="button" {{action "cancel"}}>Cancel</button>
            <button id="remove" class="btn btn-sm btn-fill" type="button" {{action "submit"}}>Remove</button>
          </div>
          <div class="clearfix"/>
        </div>
      </div>
    </div>
  </div>
{{else}}
  <BsAlert @dismissible={{false}} @type="warning">
    The prefix was not found.
  </BsAlert>
{{/if}}
