<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title" id="{{chartId}}-title">
      {{pluralize count label without-count=true}}
      <span class="small pull-right">by month</span>
    </h3>
  </div>
  <div class="panel-body panel-chart">
    <h2 class="member graphs">
      {{#if model}}
        <LinkTo @route={{link}} @model={{model.id}}>{{format-number count}}</LinkTo>
      {{else if link}}
        <LinkTo @route={{link}}>{{format-number count}}</LinkTo>
      {{else}}
        {{format-number count}}
      {{/if}}
      {{#unless cumulative}}<span class="small">in 2019</span>{{/unless}}
    </h2>
    <div class="graphs" id={{chartId}}></div>
  </div>
</div>