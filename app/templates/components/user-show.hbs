<div class="row">
  <div class='col-md-3'>
  </div>
  <div class="col-md-9">
    <div class="panel panel-transparent">
      <div class="panel-body">
        <h5>ORCID ID</h5>
        <a href="{{this.model.orcid}}">{{this.model.orcid}}</a>

        {{#if this.model.givenName}}
          <h5>Given Name</h5>
          {{this.model.givenName}}
        {{/if}}

        {{#if this.model.familyName}}
          <h5>Family Name</h5>
          {{this.model.familyName}}
        {{/if}}
      </div>
    </div>
  </div>
</div>
