<div class="row">
  <div class="col-md-9">
    <h3 class="member-results">Media</h3>
  </div>
</div>
<div class="panel panel-transparent url">
  <div class="panel-body">
    {{#if this.model.media}}
      {{#each this.model.media as |media|}}
        <div class="row">
          <div class="col-md-2">
            <strong><media.media-type /></strong>
          </div>
          <div class="col-md-7">
            <a href="{{media.url}}">{{media.url}}</a>
          </div>
        </div>
      {{/each}}
    {{else}}
      There are no <a href="https://en.wikipedia.org/wiki/Media_type">media types</a> and corresponding URLs registered for this DOI.
    {{/if}}
  </div>
</div>
