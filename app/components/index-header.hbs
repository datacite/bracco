<h2 class="work">
  DataCite
</h2>
<div class="tab-content nav-tabs-member">
  <BsNav
    @type="tabs"
    @justified={{false}}
    @stacked={{false}}
    @fill={{false}} as |nav|
  >
    <nav.item>
      <nav.linkTo @route="index" class="nav-link">
        Info
      </nav.linkTo>
    </nav.item>
    <nav.item>
      <nav.linkTo @route="settings" class="nav-link">
        Settings
      </nav.linkTo>
    </nav.item>
    <nav.item>
      <nav.linkTo @route="providers" class="nav-link">
        Members
      </nav.linkTo>
    </nav.item>
    <nav.item>
      <nav.linkTo @route="contacts" class="nav-link">
        Contacts
      </nav.linkTo>
    </nav.item>
    {{#if (feature-flag 'showResearchers')}}
      <nav.item>
        <nav.linkTo @route="users" class="nav-link">
          Users
        </nav.linkTo>
      </nav.item>
    {{/if}}
    <nav.item>
      <nav.linkTo @route="repositories" class="nav-link">
        Repositories
      </nav.linkTo>
    </nav.item>
    <nav.item>
      <nav.linkTo @route="prefixes" class="nav-link">
        Prefixes
      </nav.linkTo>
