{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": false,
    "skipLibCheck": true,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "es6",
    "experimentalDecorators": true,
    "paths": {
      "bracco/tests/*": [
        "tests/*"
      ],
      "bracco/*": [
        "app/*"
      ],
      "*": [
        "types/*"
      ]
    },
    "types": [
      "ember-source/types",
      "./node_modules/@ember-data-mirror/store/unstable-preview-types",
      "./node_modules/@ember-data-mirror/graph/unstable-preview-types",
      "./node_modules/@ember-data-mirror/json-api/unstable-preview-types",
      "./node_modules/@ember-data-mirror/request/unstable-preview-types",
      "./node_modules/@ember-data-mirror/request-utils/unstable-preview-types",
      "./node_modules/@ember-data-mirror/tracking/unstable-preview-types",
      "./node_modules/@warp-drive-mirror/core-types/unstable-preview-types",
      "./node_modules/@warp-drive-mirror/schema-record/unstable-preview-types",
      "./node_modules/@warp-drive-mirror/ember/unstable-preview-types",
      "./node_modules/@ember-data-mirror/legacy-compat/unstable-preview-types"
    ]
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "types/**/*"
  ]
}