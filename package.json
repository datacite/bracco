		"format:hbs": "./node_modules/.bin/prettier --config .prettierrc.json **/*.hbs --write",
		"format": "./node_modules/.bin/prettier --config .prettierrc.json '**/*.js|ts' --write",
		"lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
		"lint:css": "./node_modules/.bin/stylelint \"**/*.css\"",
		"lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
		"lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
		"lint:hbs": "./node_modules/.bin/ember-template-lint .",
		"lint:hbs:fix": "./node_modules/.bin/ember-template-lint . --fix",
		"lint:js": "./node_modules/.bin/eslint . --cache",
		"lint:js:fix": "./node_modules/.bin/eslint . --fix",
		"start": "FASTBOOT_DISABLED=true ./node_modules/.bin/ember serve",
		"test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
		"ci": "NODE_ENV=test ember serve & wait-on http://localhost:4200 && yarn cy:run --record --quiet",
		"cy:open": "./node_modules/.bin/cypress open",
		"cy:run": "./node_modules/.bin/cypress run",
		"postinstall": "./node_modules/.bin/patch-package"
	},
